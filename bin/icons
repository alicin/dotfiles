touch '/Applications/Sourcetree.app'

cp /Users/ali/labs/dotfiles/.icons/sourcetree__sourcetree.icns '/Applications/Sourcetree.app/Contents/Resources/sourcetree.icns'
touch '/Applications/Sourcetree.app'

cp /Users/ali/labs/dotfiles/.icons/steam__Steam.icns '/Applications/Steam.app/Contents/Resources/Steam.icns'
touch '/Applications/Steam.app'

cp /Users/ali/labs/dotfiles/.icons/steamlink__icon.icns '/Applications/Steam Link.app/Contents/Resources/icon.icns'
touch '/Applications/Steam Link.app'

